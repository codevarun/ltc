<feed xmlns="http://www.w3.org/2005/Atom">
    <title>Les Trois Couronnes</title>
    <subtitle>Didactique de l'information-documentation et identit√© professionnelle de l'enseignant documentaliste. Publications de Pascal Duplessis</subtitle>
    <id>{{ url("homepage") }}</id>
    <link href="{{ url("homepage") }}" rel="alternate" />
    <link href="{{ url("homepage") }}" />
    <updated>{{ objects[0].publishedAt|date("c") }}</updated>
    {% for obj in objects %}
    <entry>
        <title>{{ obj.title }}</title>
        {% if obj.isDoc %}
            {% set author = ltc_doc_author(obj) %}
            {% set url = ltc_doc_url(obj) %}
        {% else %}
            {% set author = obj.authorName %}
            {% set url = url('ltc_story_view', {"slug": obj.slug}) %}
        {% endif %}
          <author><name>{{ author }}</name></author>
          <updated>{{ obj.publishedAt|date("c")|raw }}</updated>
          <id>{{ url }}</id>
          <link rel="alternate" href="{{ url }}" />
          <summary>{{ obj.summary }}</summary>
    </entry>
    {% endfor %}
</feed>
